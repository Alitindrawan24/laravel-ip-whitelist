{
    "name": "alitindrawan24/laravel-ip-whitelist",
    "autoload": {
        "psr-4": {
            "Alitindrawan24\\IPWhitelist\\": "src",
            "Alitindrawan24\\IPWhitelist\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [{
        "name": "Alit Indrawan",
        "email": "alitindrawan71@gmail.com"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alitindrawan24\\IPWhitelist\\IPWhitelistServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^6.27",
        "pestphp/pest": "^1.23",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}